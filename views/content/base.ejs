<% if (page) { %>
       <% switch (page) {
              
       case 'homePage' : %>
              <%- include ('../content/dashboard') %>
              <% break;
       
       case 'authPage' : %>
              <%- include ('../content/auth/auth-content') %>
              <% break;
        
       case 'expensePage' : %>
       <%- include ('../content/expense/expense-content') %>
              <% break;
       
       case 'employeePage' : %>
       <%- include ('../content/employee/employee-content') %>
              <% break;
       
       case 'errorPage' : %>
       <%- include ('../content/error') %>
              <% break;
       
       default : %>
              <%- include ('../content/dashboard') %>
              <%  
       } %>
<% } else { %>
       <%- include ('../content/dashboard') %>
<% } %>